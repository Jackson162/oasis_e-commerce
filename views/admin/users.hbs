<h1>users page</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">email</th>
      <th scope="col">role</th>
      <th scope="col">orders</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.email}}</td>
      <td>{{this.role}}</td>
      <td><a href="/users/{{this.id}}/orders">orders</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>